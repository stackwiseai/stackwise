{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "lib": ["ES2021", "dom"],
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "exec/**/*.ts", "test/**/*.ts"]
}
